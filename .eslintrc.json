// .eslintrc.json
{
  "env": {
    "browser": true,
    "es2020": true,
    "jquery": true
  },
  "extends": [
    "plugin:@wordpress/eslint-plugin/recommended-with-formatting"
  ],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "no-console": 1,
    "jsdoc/check-line-alignment": [
      "warn",
      "never"
    ]
  }
}
